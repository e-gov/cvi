{
  "name": "veera-components",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint:all": "nx workspace-lint && nx run-many --target=lint --all",
    "affected:lint": "nx affected:lint --fix --parallel --uncommitted",
    "workspace-lint": "nx workspace-lint --uncommitted",
    "storybook:compodoc": "npx compodoc -p ./tsconfig.compodoc.json -e json -d ./libs/storybook/.storybook --disableLifeCycleHooks --disableInternal",
    "storybook": "npm run generate-icons && npm run storybook:compodoc && nx run storybook:storybook",
    "build-storybook": "nx run storybook:build-storybook",
    "prepare": "husky install",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "generate-icons": "svg-to-ts-files",
    "generate-styles": "cpy './libs/styles/src/lib/scss/**' dist/libs/styles/",
    "copy-icons-assets": "cpy './libs/icons/package.json' dist && cpy './libs/icons/README.md' dist",
    "copy-styles-assets": "cpy './libs/styles/package.json' dist && cpy './libs/styles/README.md' dist",
    "publish:styles": "npm publish dist/libs/styles",
    "publish:ui": "npm publish dist/libs/ui",
    "publish:icons": "npm publish dist/libs/icons",
    "cy:ui": "npx cypress open --project=apps/ui-e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.1.3",
    "@angular/cdk": "^14.2.0",
    "@angular/common": "~14.1.3",
    "@angular/compiler": "~14.1.3",
    "@angular/core": "~14.1.3",
    "@angular/elements": "14.1.3",
    "@angular/forms": "~14.1.3",
    "@angular/platform-browser": "~14.1.3",
    "@angular/platform-browser-dynamic": "~14.1.3",
    "@angular/router": "~14.1.1",
    "@fontsource/roboto": "^4.5.8",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@nrwl/angular": "14.8.6",
    "rxjs": "~7.5.6",
    "sanitize-html": "2.7.1",
    "svg-to-ts": "^8.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.2.10",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "~14.1.1",
    "@angular/language-service": "~14.1.1",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-angular": "17.1.0",
    "@compodoc/compodoc": "^1.1.19",
    "@jscutlery/semver": "2.26.0",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/storybook": "^14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/angular": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "cpy-cli": "^4.2.0",
    "cypress": "10.8.0",
    "cypress-storybook": "^0.5.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "jest-junit": "^14.0.1",
    "jest-preset-angular": "~11.1.2",
    "ng-packagr": "14.2.0",
    "nx": "14.5.4",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "storybook-addon-pseudo-states": "^1.15.1",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2",
    "webpack": "^5.64.0"
  }
}
